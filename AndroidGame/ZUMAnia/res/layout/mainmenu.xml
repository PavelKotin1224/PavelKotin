<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/main_menu_frame" android:orientation="vertical"
	android:layout_width="fill_parent" android:layout_height="fill_parent">
	<com.zumania.MainPage
		android:id="@+id/main_menu_page" android:layout_height="fill_parent"
		android:layout_width="fill_parent">
	</com.zumania.MainPage>

	<LinearLayout android:id="@+id/LinearLayout01"
		android:layout_width="fill_parent" android:orientation="vertical"
		android:layout_height="fill_parent" android:background="@drawable/page_background">
		<LinearLayout android:layout_width="fill_parent" android:id="@+id/linearLayout1" android:gravity="center_vertical|center_horizontal" android:layout_weight="10" android:layout_height="wrap_content">
			<LinearLayout android:layout_width="wrap_content" android:id="@+id/linearLayout11" android:layout_height="fill_parent" android:layout_weight="2"></LinearLayout>
			<ImageView android:id="@+id/imageView1" android:src="@drawable/main_icon" android:layout_weight="1" android:layout_width="wrap_content" android:layout_height="wrap_content"></ImageView>
			<LinearLayout android:layout_width="wrap_content" android:id="@+id/linearLayout12" android:layout_height="fill_parent" android:layout_weight="2"></LinearLayout>
		</LinearLayout>
		<LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/linearLayout2" android:layout_weight="20" android:orientation="vertical">
			<LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/linearLayout7" android:layout_weight="1" android:gravity="center_vertical|center_horizontal">
				<com.component.CustomImageButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/start_button" android:id="@+id/start_button"></com.component.CustomImageButton>
			</LinearLayout>
			<LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/linearLayout8" android:layout_weight="1" android:gravity="center_vertical|center_horizontal">
				<com.component.CustomImageButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/option_button" android:id="@+id/option_button"></com.component.CustomImageButton>
			</LinearLayout>
			<LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/linearLayout9" android:layout_weight="1" android:gravity="center_vertical|center_horizontal">
				<com.component.CustomImageButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/score_button" android:id="@+id/score_button"></com.component.CustomImageButton>
			</LinearLayout>
			<LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/linearLayout10" android:layout_weight="1" android:gravity="center_vertical|center_horizontal">
				<com.component.CustomImageButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/about_button" android:id="@+id/about_button"></com.component.CustomImageButton>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/linearLayout3" android:layout_weight="10">
			<LinearLayout android:layout_height="fill_parent" android:layout_width="wrap_content" android:id="@+id/linearLayout4" android:gravity="bottom|center_horizontal" android:layout_weight="2">
				<com.component.CustomImageButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/exit_button" android:id="@+id/exit_button"></com.component.CustomImageButton>
			</LinearLayout>
			<LinearLayout android:layout_height="fill_parent" android:layout_width="wrap_content" android:id="@+id/linearLayout5" android:layout_weight="7">
			</LinearLayout>
			<LinearLayout android:layout_height="fill_parent" android:layout_width="wrap_content" android:id="@+id/linearLayout6" android:layout_weight="2" android:gravity="bottom|center_horizontal">
				<com.component.CustomImageButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/help_button" android:id="@+id/help_button"></com.component.CustomImageButton>
			</LinearLayout>
		</LinearLayout>




















	</LinearLayout>


</FrameLayout>
