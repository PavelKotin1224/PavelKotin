<?xml version="1.0" encoding="utf-8"?>

<LinearLayout android:id="@+id/LinearLayout01" xmlns:android="http://schemas.android.com/apk/res/android" android:background="@drawable/grass_background" android:orientation="vertical" android:layout_width="230dp" android:layout_height="170dp">
    <LinearLayout android:id="@+id/linearLayout1" android:layout_weight="1" android:gravity="center_vertical|center_horizontal" android:layout_width="fill_parent" android:layout_height="fill_parent">
        <ImageView android:id="@+id/imageView1" android:layout_width="wrap_content" android:src="@drawable/exit_text" android:layout_height="wrap_content"></ImageView>
    </LinearLayout>
    <LinearLayout android:layout_width="fill_parent" android:id="@+id/linearLayout2" android:layout_weight="1" android:orientation="horizontal" android:layout_height="fill_parent">
        <LinearLayout android:layout_height="fill_parent" android:layout_width="wrap_content" android:id="@+id/linearLayout3" android:layout_weight="1" android:gravity="top|center_horizontal">
            <com.component.CustomImageButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/yes_button" android:id="@+id/exitok"></com.component.CustomImageButton>
        </LinearLayout>
        <LinearLayout android:layout_height="fill_parent" android:layout_width="wrap_content" android:id="@+id/linearLayout4" android:layout_weight="1" android:gravity="top|center_horizontal">
            <com.component.CustomImageButton android:layout_height="wrap_content" android:layout_width="wrap_content" android:background="@drawable/no_button" android:id="@+id/exitcancel"></com.component.CustomImageButton>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
